{
  "name": "@tectalic/openai",
  "version": "0.2.1",
  "description": "Tectalic OpenAI REST API Client",
  "homepage": "https://tectalic.com/apis/openai",
  "keywords": [
    "ai",
    "api",
    "gpt-3",
    "gpt3",
    "openai",
    "rest",
    "tectalic"
  ],
  "author": "Tectalic",
  "license": "MIT",
  "main": "dist//index.js",
  "types": "/index.d.ts",
  "engines": {
    "node": ">=16.0",
    "npm": ">=8.0"
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "scripts": {
    "build": "npm run clean && tsc && npm run format:dist",
    "clean": "rm -rf ./dist/* ./coverage/*",
    "format:dist": "prettier --write dist && semistandard --fix dist || true",
    "format:src": "prettier --write src",
    "test": "jest",
    "test:coverage": "npm run test:unit --  --collectCoverage",
    "test:integration": "jest test/integration",
    "test:unit": "jest test/unit"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "form-data": "^4.0.0",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.3",
    "@types/qs": "^6.9.7",
    "axios-mock-adapter": "^1.21.2",
    "jest": "^29.2.2",
    "openapi-enforcer": "^1.22.0",
    "prettier": "^2.7.1",
    "semistandard": "^16.0.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
